
@{
    ViewBag.Title = "Index";
}


<h2>Index</h2>
<a href="/student/create">Create</a>
<span>@TempData["Msg"]</span>
<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Cgpa</th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Cgpa</td>
            <td>
                <a href="/student/details/@item.Id">Details</a>
                <a href="/student/edit/@item.Id">Edit</a>
                <a href="/student/delete/@item.Id">Delete</a>
            </td>
        </tr>
    }
</table>

